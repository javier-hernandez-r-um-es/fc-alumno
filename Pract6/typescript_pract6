[?2004l#include <sys/types.h>
#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>

int globalVar = -1;

void hello(void) {
    printf("Hola, mundo! Mi pid es %d\n", getpid());
}

void illegal(void) {
    __asm__ ( ".long 0xFFFFFFFF"); // Illegal instrucion
}

int main(void) {
    int localVar;
    int *heapVar = (int *)malloc(sizeof(int));
    hello();
    unsigned long f =(unsigned long)&hello;
    unsigned long p =(unsigned long)&printf;
    unsigned long i =(unsigned long)&illegal;
    unsigned long g =(unsigned long)&globalVar;
    unsigned long l =(unsigned long)&localVar;
    unsigned long h =(unsigned long)heapVar;
    /******************************************/
    printf("Direcci√≥n de 'f'  %lx\n", f);
    printf("Direcci√≥n de 'p'  %lx\n", p);
    printf("Direcci√≥n de 'g'  %lx\n", g);
    printf("Direcci√≥n de 'l'  %lx\n", l);
    printf("Direcci√≥n de 'h'  %lx\n", h);
    printf("Direcci√≥n de 'i'  %lx\n", i);
    printf("Pulsa INTRO para continuar..");
    getchar();
    unsigned long testAddress = 0;
    printf("Introduce una direcci√≥n (hex): ");
    scanf("%lx", &testAddress);
    printf("Opciones: 'r' (leer), 'w' (escribir), 'x' (ejecutar)\n");
    printf("Elige una opci√≥n: ");
    char opcion;
    scanf(" %c", &opcion); // Leading blank to skip newline in input buffer.
    if (opcion == 'r') {
        printf("Voy a leer 4 bytes a partir de la direcci√≥n %lx...\n", testAddress);
        for(int i=0; i < sizeof(int);i++) {
            unsigned char valor  = *((unsigned char *) testAddress+i);
            printf(" %02x", (unsigned int)valor);
        }
        printf("\n");
    }else if (opcion  == 'w') {
        printf("Voy a escribir 4 bytes a partir de la direcci√≥n %lx...\n", testAddress);
        *((unsigned long *) testAddress) = 0;
    }else if (opcion  == 'x') {
        printf("Voy a ejecutar a partir de la direcci√≥n %lx...\n", testAddress);
        void (*foo)(void) = (void (*)())testAddress;
        foo();
    }else {
        printf("Opci√≥n inv√°lida\n");
    }
    printf("Terminado con √©xito\n");    
}
